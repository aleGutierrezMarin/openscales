<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("org.openscales.sld.ui.HandlersManager")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>

	<s:List id="content"
			left="0"
			right="0"
			top="0"
			bottom="0"
			borderAlpha="0"
			contentBackgroundAlpha="0"
			rollOverColor="0xffffff"
			dataProvider="{this.hostComponent.contentElements}"
			itemRenderer="org.openscales.sld.skin.DefaultHandlerItemRenderer">
		<!--<s:layout>
			<s:VerticalLayout gap="5" />
		</s:layout>-->
	</s:List>
	
</s:Skin>
